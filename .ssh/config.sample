Host <host_you_want_as>
    identityfile ~/.private/ssh/<private_key_file>
    hostname example.com
    user <username>

Host *
    controlpath ~/.ssh/master-%r@%h:%p
    controlmaster auto

Host *.amazonaws.com 
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  User ubuntu
  LogLevel QUIET
